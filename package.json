{
	"name": "bvwp",
	"version": "1.0.7",
	"last_version": "1.0.6",
	"scripts": {
		"buildf": "npm-run-all --sequential twbuildf jsbuildf",
		"twbuildf": "tailwindcss -i ./src/frontend.css -o ./css/infinite-theme.css --postcss --minify",
		"jsbuildf": "uglifyjs-folder ./src/js/ -o ./js/ -e -x .js",
		"buildb": "npm-run-all --sequential twbuildb jsbuildb",
		"twbuildb": "tailwindcss -i ./src/blocks.css -o ./css/infinite-blocks.css --postcss --minify",
		"jsbuildb": "uglifyjs-folder ./src/js/ -o ./js/ -e -x .js",
		"devf": "npm-run-all --parallel twdevf jsdevf",
		"twdevf": "tailwindcss -i ./src/frontend.css -o ./css/infinite-theme.css --watch --postcss",
		"jsdevf": "watch 'npm run jsbuildf' ./src/js/",
		"devb": "npm-run-all --parallel twdevb jsdevb",
		"twdevb": "tailwindcss -i ./src/blocks.css -o ./css/infinite-blocks.css --watch --postcss",
		"jsdevb": "watch 'npm run jsbuildb' ./src/js/",
		"grunt": "grunt default",
		"dev": "npm-run-all --sequential devf devb",
		"build": "npm-run-all --sequential buildf buildb",
		"dist": "npm-run-all --sequential buildf buildb grunt"
	},
	"devDependencies": {
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"autoprefixer": "^10.0.1",
		"clean-css": "^5.3.3",
		"grunt": "^1.6.1",
		"grunt-contrib-compress": "^2.0.0",
		"grunt-string-replace": "^1.3.3",
		"npm-run-all": "^4.1.5",
		"postcss": "^8",
		"postcss-nested": "^6.0.1",
		"postcss-prefix-selector": "^1.16.0",
		"tailwindcss": "^3.3.0",
		"uglify-js": "^3.17.4",
		"uglifyjs-folder": "^3.3.0",
		"watch": "^0.13.0"
	}
}
